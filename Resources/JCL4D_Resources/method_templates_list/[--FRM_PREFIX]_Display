//[--FRM_PREFIX]_Display
//FG v202402 [--DATE]
//[--TABLE] 一覧ウインドウ表示

C_LONGINT($wref)
C_LONGINT($0)
C_TEXT($title)
C_TEXT($frmName)
$frmName:="[--FRM_PREFIX]_List"

$wref:=Open form window([[--TABLE]];$frmName;Plain form dialog box;Horizontally Centered;Vertically Centered;*)

$title:=$frmName
SET WINDOW TITLE($title; $wref)

DIALOG([[--TABLE]];$frmName)
CLOSE WINDOW($Wref)//ウィンドウを閉じる 

$0:=OK
